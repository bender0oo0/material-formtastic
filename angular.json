{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "cli": {
    "schematicCollections": [
      "@angular-eslint/schematics"
    ]
  },
  "projects": {
    "material-formtastic": {
      "projectType": "library",
      "root": "projects/material-formtastic",
      "sourceRoot": "projects/material-formtastic/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/material-formtastic/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/material-formtastic/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/material-formtastic/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:jest",
          "options": {
            "tsConfig": "projects/material-formtastic/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/material-formtastic/src/**/*.ts",
              "projects/material-formtastic/src/**/*.html"
            ]
          }
        }
      }
    },
    "samples": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/samples",
      "sourceRoot": "projects/samples/src",
      "prefix": "sample",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/samples",
            "index": "projects/samples/src/index.html",
            "browser": "projects/samples/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/samples/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/samples/src/favicon.ico",
              "projects/samples/src/assets"
            ],
            "styles": [
              "projects/samples/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "samples:build:production"
            },
            "development": {
              "buildTarget": "samples:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "samples:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:jest",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/samples/tsconfig.spec.json"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": [
              "projects/samples/src/**/*.ts",
              "projects/samples/src/**/*.html"
            ]
          }
        }
      }
    }
  }
}
